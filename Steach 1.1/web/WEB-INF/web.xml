        <servlet-class>Controlador.ServletAmistad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletPublica</servlet-name>
        <servlet-class>Controlador.ServletPublica</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletPuertaPerfil</servlet-name>
        <servlet-class>Controlador.ServletPuertaPerfil</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletPrincipal</servlet-name>
        <url-pattern>/ServletPrincipal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletRegistrar</servlet-name>
        <url-pattern>/ServletRegistrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletIniciarSesion</servlet-name>
        <url-pattern>/ServletIniciarSesion</url-pattern>
    </servlet-mapping>
