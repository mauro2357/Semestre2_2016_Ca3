        <servlet-name>ServletAmistad</servlet-name>
        <url-pattern>/ServletAmistad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletPublica</servlet-name>
        <url-pattern>/ServletPublica</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletPuertaPerfil</servlet-name>
        <url-pattern>/ServletPuertaPerfil</url-pattern>
    </servlet-mapping>
